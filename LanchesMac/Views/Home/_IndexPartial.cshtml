<div class="row no-gutter">
    @{
         string[] imgs = {
                        "~/images/carousel11.jpg",
                        "~/images/carousel12.jpg",
                        "~/images/carousel13.jpg",
                        "~/images/carousel14.jpg",
                        "~/images/carousel17.jpg"
             };
    }

    @foreach (var img in imgs)
    {
        <div class="col-xs-2 col-md-2">
            <a class="portfolio-box" asp-action="List" asp-controller="Lanche">
                <img src="@Url.Content(img)" alt="Lanches" width="450" height="220"/>
            </a>
        </div>
    }
</div>

<br>

<h2>Os preferidos da semana </h2>
<h4>Tá com fome ???</h4>

<div class="row">
    @foreach (var lanche in Model.LanchesPreferidos)
    {
        @*@Html.Partial("_LanchesResumo", lanche)*@
        <partial name="_LanchesResumo" for="@lanche" />
    }
</div>